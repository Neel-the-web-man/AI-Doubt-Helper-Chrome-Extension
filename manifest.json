{
  "manifest_version": 3,
  "name": "AI DSA Helper",
  "version": "0.1",
  "author": "Neel Kotkar",
  "description": "You can ask doubts to page specifc coding problem and AI response will help you ,improving overall user experience.",
  "permissions": [
    "storage",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://maang.in/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources" : [
    {
      "resources": [
        "assets/images/chatbot.png",
        "assets/images/idea.png",
        "assets/images/send-message.png"
      ],
      "matches" : ["<all_urls>"]
    }
  ],
  "action": {
    "default_icon": {
      "16": "assets/images/idea.png",
      "24": "assets/images/idea.png",
      "32": "assets/images/idea.png"
    },
    "default_popup": "popup.html",
    "default_title": "Ai Chatbot helper."
  }
}